		"editorGroup.dropBackground":"#47047ab4",
		"editorGroupHeader.noTabsBackground": "#140024",

		//Activity Bar
		"activityBar.background":"#18012a",
		"activityBar.dropBackground": "#2f03539d",
		"activityBar.foreground":"#fff",
		"activityBar.inactiveForeground":"#c8acec",
		"activityBar.border":"#1a012ef3",
		"activityBarBadge.background": "#33035a",
		"activityBarBadge.foreground":"#fff",
		
